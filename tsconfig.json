{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["DOM", "ES2017"],
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["vite/client"]
  },
  "exclude": ["node_modules/@types/prop-types"],
  "include": ["src"]
}
